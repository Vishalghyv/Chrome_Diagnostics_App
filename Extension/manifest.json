{
  "name": "System Information",
  "version": "1.0",
  "description": "Extension to fetch System Information ",
  "permissions": [
    "system.cpu",
    "system.memory",
    "system.storage",
    "system.display",
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "externally_connectable": {
    "matches": ["*://chrome-diagnostics-app.web.app/*", "http://localhost:5000/"]
  }
}
